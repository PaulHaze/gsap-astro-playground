---
import BaseLayout from '@/layouts/BaseLayout.astro';

import { BoxExample } from '@/components/gsap/react/BoxExample.tsx';
---

<BaseLayout title="Easing" sideBarActiveItemID="easing-react">
	<main class="space-y-8">
		<BoxExample client:only="react" target="power1-in" easingType="power1.in" />
		<BoxExample
			client:only="react"
			target="power1-out"
			easingType="power1.out"
		/>
		<BoxExample
			client:only="react"
			target="power1-inOut"
			easingType="power1.inOut"
		/>
	</main>
</BaseLayout>
